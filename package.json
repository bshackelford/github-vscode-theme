{
  "name": "bshack-vscode-theme",
  "displayName": "BShack Theme",
  "description": "BShackâ€™s theme for VS Code",
  "version": "1.0.0",
  "publisher": "BShack",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#121212",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bshackelford/github-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/bshackelford/github-vscode-theme/issues"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "light",
    "dark",
    "protanopia",
    "colorblind"
  ],
  "contributes": {
    "themes": [
      {
        "label": "BShack Light",
        "uiTheme": "vs",
        "path": "./themes/bshack-light.json"
      },
      {
        "label": "BShack Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/bshack-dark.json"
      }
    ]
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.1",
    "@changesets/cli": "^2.17.0",
    "@primer/primitives": "7.10.0",
    "chroma-js": "^2.1.2",
    "color": "^3.1.2",
    "nodemon": "^2.0.3",
    "vsce": "^1.100.1"
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js && mkdir -p build",
    "package": "vsce package -o ./build/bshack-vscode-theme.vsix",
    "prepublishOnly": "yarn build && vsce publish",
    "release": "changeset publish"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
